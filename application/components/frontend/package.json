    "connected-react-router": "^6.8.0",
    "dompurify": "^2.0.12",
    "express": "^4.17.1",
    "history": "^4.10.1",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "morgan": "^1.10.0",
    "nocache": "^2.1.0",
    "promise": "8.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dev-utils": "^10.2.1",
    "react-dom": "^16.13.1",
    "react-loadable": "^5.5.0",
    "react-onclickoutside": "^6.9.0",
    "react-rangeslider": "^2.2.0",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-router-redux": "^4.0.8",
    "react-step-wizard": "^5.3.2",
    "react-svg-map": "^2.1.2",
    "react-transition-group": "^4.4.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "shortid": "^2.2.15",
    "styled-components": "^5.1.1",
    "whatwg-fetch": "3.2.0",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.18.2",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.2.2",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "chalk": "^4.1.0",
    "concurrently": "^5.2.0",
    "connect-history-api-fallback": "^1.6.0",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^4.1.1",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.2.2",
    "jest-sonar-reporter": "^2.0.0",
    "jest-styled-components": "^7.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^2.0.5",
    "redux-mock-store": "^1.5.4",
    "style-loader": "^1.2.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-manifest-plugin": "^2.2.0"
  },
  "scripts": {
    "start": "node --max-http-header-size=48000 config/webpackDevServer.js",
    "build": "concurrently \"yarn build:front\" \"yarn build:server\"",
    "build:front": "webpack --config config/webpack.config.prod.js",
    "build:server": "webpack --config config/webpack.config.server.js",
    "test": "jest --env=jsdom --coverage",
    "test:watch": "jest --env=jsdom --watch",
    "lint": "eslint --ext js,jsx src"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/jest/testSetup.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx}"
    ],
    "testResultsProcessor": "jest-sonar-reporter",
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "jestSonar": {
    "reportPath": "build/test-results",
    "reportFile": "jest.xml"
  }
}
